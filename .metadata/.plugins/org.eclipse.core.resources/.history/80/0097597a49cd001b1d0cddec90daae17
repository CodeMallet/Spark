			// Down Collision Tiles
			case 422:
			case 423:
			case 424:
			case 466:
			case 467:
			case 806:
			case 807:
			case 808:
			case 809:
			case 813:
			case 3205:
			case 3206:
			case 3207:
			case 3208:
			case 3212:
			{
				if ((Angle * 180 / Math.PI) > -180
						&& (Angle * 180 / Math.PI) < 0)
				{

					return true;

				}
			}
		}
		
		switch (tile)
		{
			// Left Collision Tiles
			case 345:
			case 385:
			case 425:
			case 730:
			case 770:
			case 810:
			case 3129:
			case 3169:
			case 3209:
			{
				if ((Angle * 180 / Math.PI) > -90
						&& (Angle * 180 / Math.PI) < 90)
				{
					return true;
				}
			}
		}
		
		switch (tile)
		{
			// Right Collision Tiles
			case 308:
			case 348:
			case 388:
			case 693:
			case 733:
			case 773:
			case 3092:
			case 3132:
			case 3172:
			{
				if ((Angle * 180 / Math.PI) < -90
						|| (Angle * 180 / Math.PI) > 90)
				{
					return true;

				}
			}
		}

		return false;
	}
}
